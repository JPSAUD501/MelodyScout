// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider                = "prisma-client-js"
}

datasource db {
  provider                = "postgresql"
  url                     = env("DATABASE_URL")
}

model TrackerChats {
  chatId                  String @id @unique
  trackingUsers           TrackingUsers[]
}

model TrackingUsers {
  lastfmUser              String @id @unique
  trackingInChats         TrackerChats[]
}

model TelegramUsers {
  telegramUserId          String @id @unique
  lastfmUser              String?
  lastUpdate              String
}

