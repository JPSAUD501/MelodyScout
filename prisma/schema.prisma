generator client {
  provider                = "prisma-client-js"
}

datasource db {
  provider                = "postgresql"
  url                     = env("DATABASE_URL")
}

model TelegramUsers {
  telegramUserId          String @id @unique
  lastfmUser              String?
  lastUpdate              String
}

