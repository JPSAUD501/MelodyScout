{
  "name": "melodyscout",
  "version": "1.0.0",
  "description": "Last.fm Telegram Tracker bot",
  "main": "app.ts",
  "scripts": {
    "start:prod": "yarn deploy && yarn start",
    "start": "yarn ts-node --transpile-only ./app.ts",
    "start:dev": "yarn ts-node ./app.ts",
    "deploy": "yarn prisma migrate deploy",
    "dev": "yarn translation && yarn start:dev",
    "translation": "yarn ts-node ./translations/runModule.ts",
    "test": "jest",

    "beta:build": "yarn && yarn ncc build --transpile-only ./app.ts -o ./dist && yarn ts-node ./functions/fixDist.ts",
    "beta:start": "node ./dist/index.js",
    "beta:start:prod": "yarn deploy && yarn beta:start"
  },
